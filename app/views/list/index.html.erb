 <h6>データ取得日時  <%= @latestOne.updated_at.strftime("%Y年%m月%d日 %H時%M分")%></h6> 
<div class="panel panel-primary">
  <div class="panel-heading">相場データ</div>
  <!-- Table -->
  <div class="table-responsive">
    <table class="table table-striped table-bordered table-hover">
<thead>
    <tr>
      <th>取引所</th>
      <th>買い価格</th>
      <th>売り価格</th>
    </tr>
  </thead>

  <tbody>
    <% @prices.each do |price| %>
      <tr>
        <td><%= price.name %></td>
        <td><%= price.buy_price.round %></td>
        <td><%= price.sell_price.round %></td>
      </tr>
    <% end %>
  </tbody>
 </table>
  </div><!--/table-responsive-->
</div><!--/panel-->


<h4>日本のビットコイン取引所ランキング</h4>

<ul class="list-group">
	<% @serviceLinks.each do |service| %>
		<li class="list-group-item">
			<h4><%= service.name %></h4>
			<h5><%= service.memo %></h5>
			<%= service.link.html_safe %>
		</li>
  	<% end %>
</ul>